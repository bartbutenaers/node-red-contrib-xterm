    "dependencies": {
       "xterm": "4.2.0-vscode1",
       "xterm-addon-fit": "0.3.0",
       "node-pty-prebuilt-multiarch": "0.9.0"
    },
    "author": {
        "name": "Bart Butenaers"
    },
    "license": "Apache-2.0",
    "keywords": [ "node-red", "terminal", "linux", "dos", "cli", "command line", "command" ],
    "bugs": {
        "url": "https://github.com/bartbutenaers/node-red-contrib-xterm/issues"
    },
    "homepage": "https://github.com/bartbutenaers/node-red-contrib-xterm",
    "repository": {
        "type": "git",
        "url": "https://github.com/bartbutenaers/node-red-contrib-xterm.git"
    },
    "node-red"     : {
